@model DevCenterGallary.Common.Models.Product
@{
    ViewData["Title"] = "Submissions";
    var productId = ViewData["productId"];
}
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a asp-action="Products">Products</a></li>
        <li class="breadcrumb-item active"><a href="#">@Model.Name</a></li>
    </ol>
    <div class="text-left">
        <ul class="list-group">
            @foreach (var item in Model.Submissions)
            {
                <li class ="list-group-item">
                    <a asp-action="Packages" asp-route-productId="@Model.BigId" asp-route-submissionId="@item.Id">@item.FriendlyName</a>
                </li>
                }
        </ul>
    </div>
